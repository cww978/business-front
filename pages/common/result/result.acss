.result {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 10vh;
}
.result-icon image{
  width: 180rpx;
  height: 180rpx;
  margin: 8px;
}
.result-title, .result-des {
  margin: 8px
}
.result-des {
  width: 70%;
}
.result-action {
  bottom: 30vh;
  position: absolute;
  display: flex;
  justify-content: center;
  width: 100%;
}
.result-action button {
  width: 80%;
  height: 40px;
  line-height: 40px;
}