.loading-wapper{
  position: absolute;
  z-index: 9;
  height: 100vh;
  background-color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
}
.loading-content{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.loading-wapper-show {
  display: flex
}
.loading-wapper-hide {
  display: none;
}